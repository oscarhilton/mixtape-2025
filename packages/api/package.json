    "test": "jest"
  },
  "dependencies": {
    "@types/node-fetch": "2",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-session": "^1.18.1",
    "knex": "^3.1.0",
    "node-fetch": "2",
    "passport": "^0.7.0",
    "passport-spotify": "^2.0.0",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.1",
    "@types/express-session": "^1.18.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.18",
    "@types/passport": "^1.0.17",
    "@types/passport-spotify": "^2.0.4",
    "@types/supertest": "^6.0.3",
    "jest": "^29.7.0",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.4",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  }
